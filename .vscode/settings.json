{
  "cSpell.enabled": true,
  "editor.tabSize": 2,
  "editor.useTabStops": true,
  "editor.stickyTabStops": true,
  "todo-list.enableTelemetry": false,
  "todo-list.expression": "(?://|#)\\s+([A-Z]+):\\s+(.+)$",
  "todo-list.include": "**/*.{ts,js,php,css,scss,html,dart,md,yaml}",
  "todo-list.exclude": "{**/dist/**,**/build/**,**/.vscode/**,**/_output/**,**/vendor/**,**/*.min.*,**/*.map,**/.dart_tool/**}",
  "files.eol": "\n",
  "[dart]": {
    "editor.defaultFormatter": "Dart-Code.dart-code",
    "editor.formatOnSave": true,
  }
}
